          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "\r\nSELECT\r\n  f.fc_time,\r\n  MAX(CASE WHEN dp.id = 9 THEN value END) AS wind_direction,\r\n  MAX(CASE WHEN dp.id = 10 THEN value END) AS wind_speed\r\nFROM public.forecasts f\r\nJOIN public.data_points dp ON f.dp_id = dp.id\r\nWHERE dp.id IN (9, 10)\r\n  AND $__timeFilter(f.fc_time)\r\nGROUP BY f.fc_time\r\nORDER BY f.fc_time;\r\n",
