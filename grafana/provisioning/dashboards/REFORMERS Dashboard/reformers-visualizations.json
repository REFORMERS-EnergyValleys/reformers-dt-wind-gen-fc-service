          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  f.fc_time,\r\n  value AS output,\r\n  CASE dp.id\r\n    WHEN 5 THEN 'turbine 1'\r\n    WHEN 6 THEN 'turbine 2'\r\n    WHEN 7 THEN 'turbine 3'\r\n    WHEN 8 THEN 'turbine 4'\r\n  END AS turbine_name\r\nFROM public.forecasts f\r\nJOIN public.data_points dp ON f.dp_id = dp.id\r\nWHERE dp.id IN (5, 6, 7, 8)\r\n  AND $__timeFilter(f.fc_time)\r\nORDER BY f.fc_time, dp.id;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "turbine power outputs",
      "transformations": [
        {
          "id": "partitionByValues",
          "options": {
            "fields": [
              "turbine_name"
            ]
          }
        }
      ],
      "type": "timeseries"
    },
