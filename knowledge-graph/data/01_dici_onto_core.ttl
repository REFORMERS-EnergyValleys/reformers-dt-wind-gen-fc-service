@prefix dici_core: <urn:digicities:core#> .
@prefix fibo-currency: <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<urn:digicities:core> a owl:Ontology .

dici_core:Author a owl:AnnotationProperty .

dici_core:Collaborator a owl:AnnotationProperty .

dici_core:Institution a owl:AnnotationProperty .

dici_core:Synonymous a owl:AnnotationProperty .

dici_core:abbreviation a owl:AnnotationProperty .

dici_core:definition a owl:AnnotationProperty .

dici_core:hasDefaultUnit a owl:AnnotationProperty ;
                         rdfs:range qudt:Unit ;
                         rdfs:domain dici_core:Attribute .

dici_core:hasQuantityKind a owl:AnnotationProperty ;
                          rdfs:range qudt:QuantityKind ;
                          rdfs:domain dici_core:Attribute .

xsd:duration a rdfs:Datatype .

qudt:unit a owl:ObjectProperty ;
          rdfs:subPropertyOf dici_core:hasUnit ;
          rdfs:domain dici_core:Attribute ;
          rdfs:range qudt:Unit .

dici_core:actuates a owl:ObjectProperty ;
                   rdfs:subPropertyOf dici_core:controls ;
                   rdfs:domain dici_core:Actuator ;
                   rdfs:range dici_core:Component ;
                   rdfs:comment "Actuator physically controls a component"@en ;
                   rdfs:label "actuates" .

dici_core:assumptionObjectProperty a owl:ObjectProperty .

dici_core:carriesEnergyCarrier a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:carriesResource ;
                               rdfs:domain dici_core:EnergyCarrierFlow ;
                               rdfs:range dici_core:EnergyCarrier ;
                               rdfs:label "carries energy carrier" .

dici_core:carriesResource a owl:ObjectProperty ;
                          rdfs:domain dici_core:Flow ;
                          rdfs:range dici_core:Resource ;
                          rdfs:comment "The resource (energy carrier or material) being transported by the flow"@en ;
                          rdfs:label "carries resource" .

dici_core:controls a owl:ObjectProperty ;
                   rdfs:subPropertyOf dici_core:linksComponent ;
                   rdfs:domain dici_core:Device ;
                   rdfs:range dici_core:Component ;
                   rdfs:comment "Device controls another component"@en ;
                   rdfs:label "controls" .

dici_core:currency a owl:ObjectProperty ;
                   rdfs:domain dici_core:CostAttribute ;
                   rdfs:range fibo-currency:Currency ;
                   rdfs:label "currency" .

dici_core:derivedFrom a owl:ObjectProperty ;
                      rdfs:domain dici_core:Scenario ;
                      rdfs:range dici_core:Scenario .

dici_core:fedBy a owl:ObjectProperty ;
                rdfs:subPropertyOf dici_core:linksComponent ;
                owl:inverseOf dici_core:feeds ;
                rdfs:domain dici_core:Component ;
                rdfs:range dici_core:Component .

dici_core:feeds a owl:ObjectProperty ;
                rdfs:subPropertyOf dici_core:linksComponent .

dici_core:flowsThrough a owl:ObjectProperty ;
                       rdfs:subPropertyOf dici_core:linksComponent ;
                       rdfs:domain dici_core:Flow ;
                       rdfs:range dici_core:Location ;
                       rdfs:comment "Locations traversed by a flow"@en ;
                       rdfs:label "flows through" .

dici_core:hasActorAttribute a owl:ObjectProperty ;
                            rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                            rdfs:domain dici_core:Actor ;
                            rdfs:range dici_core:ActorAttribute ;
                            rdfs:label "has actor attribute" .

dici_core:hasActuatorAttribute a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:hasDeviceAttribute ;
                               rdfs:domain dici_core:Actuator ;
                               rdfs:range dici_core:ActuatorAttribute ;
                               rdfs:label "has actuator attribute" .

dici_core:hasAttribute a owl:ObjectProperty ;
                       rdfs:domain dici_core:Component ;
                       rdfs:range dici_core:Attribute ;
                       rdfs:label "has Attribute" .

dici_core:hasColdAttribute a owl:ObjectProperty ;
                           rdfs:subPropertyOf dici_core:hasThermalEnergyAttribute ;
                           rdfs:domain dici_core:ColdCarrier .

dici_core:hasComponentAttribute a owl:ObjectProperty ;
                                rdfs:subPropertyOf dici_core:hasAttribute .

dici_core:hasControllerAttribute a owl:ObjectProperty ;
                                 rdfs:subPropertyOf dici_core:hasDeviceAttribute ;
                                 rdfs:domain dici_core:Controller ;
                                 rdfs:range dici_core:ControllerAttribute ;
                                 rdfs:label "has controller attribute" .

dici_core:hasConversionProcessAttribute a owl:ObjectProperty ;
                                        rdfs:subPropertyOf dici_core:hasProcessAttribute ;
                                        rdfs:domain dici_core:ConversionProcess ;
                                        rdfs:range dici_core:ConversionProcessAttribute ;
                                        rdfs:label "has conversion process attribute" .

dici_core:hasConverterAttribute a owl:ObjectProperty ;
                                rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                                rdfs:domain dici_core:Converter .

dici_core:hasDataPoints a owl:ObjectProperty ;
                        rdfs:label "has data points" ;
                        rdfs:domain dici_core:CurveAttribute ;
                        rdfs:range rdf:JSON .

dici_core:hasDestination a owl:ObjectProperty ;
                         rdfs:subPropertyOf dici_core:linksComponent ;
                         owl:inverseOf dici_core:isDestinationOf ;
                         rdfs:domain dici_core:Flow ;
                         rdfs:range dici_core:Component ;
                         rdfs:comment "The component to which the flow is directed"@en ;
                         rdfs:label "has destination" .

dici_core:hasDeviceAttribute a owl:ObjectProperty ;
                             rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                             rdfs:domain dici_core:Device ;
                             rdfs:range dici_core:DeviceAttribute ;
                             rdfs:label "has device attribute" .

dici_core:hasElectricityAttribute a owl:ObjectProperty ;
                                  rdfs:subPropertyOf dici_core:hasEnergyCarrierAttribute ;
                                  rdfs:domain dici_core:ElectricityCarrier .

dici_core:hasEnergyCarrierAttribute a owl:ObjectProperty ;
                                    rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                                    rdfs:domain dici_core:EnergyCarrier .

dici_core:hasEnergyCarrierEnergyCostAttribute a owl:ObjectProperty ;
                                              rdfs:subPropertyOf dici_core:hasEnergyCarrierAttribute ;
                                              rdfs:range dici_core:EnergyCostAttribute .

dici_core:hasEnergyCarrierFlowAttribute a owl:ObjectProperty ;
                                        rdfs:subPropertyOf dici_core:hasFlowAttribute ;
                                        rdfs:domain dici_core:EnergyCarrierFlow ;
                                        rdfs:range dici_core:EnergyCarrierFlowAttribute ;
                                        rdfs:label "has energy carrier flow attribute" .

dici_core:hasEnergyConsumerAttribute a owl:ObjectProperty ;
                                     rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                                     rdfs:domain dici_core:EnergyConsumer .

dici_core:hasEnergyConverterAttribute a owl:ObjectProperty ;
                                      rdfs:subPropertyOf dici_core:hasConverterAttribute ;
                                      rdfs:domain dici_core:EnergyConverter .

dici_core:hasFlowAttribute a owl:ObjectProperty ;
                           rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                           rdfs:domain dici_core:Flow ;
                           rdfs:range dici_core:FlowAttribute ;
                           rdfs:label "has flow attribute" .

dici_core:hasFuelAttribute a owl:ObjectProperty ;
                           rdfs:subPropertyOf dici_core:hasEnergyCarrierAttribute ;
                           rdfs:domain dici_core:FuelCarrier .

dici_core:hasFutureTimeSeries a owl:ObjectProperty ;
                              rdfs:subPropertyOf dici_core:hasTimeSeries ;
                              owl:inverseOf dici_core:isFutureTimeSeriesOf .

dici_core:hasGaseousFuelAttribute a owl:ObjectProperty ;
                                  rdfs:subPropertyOf dici_core:hasFuelAttribute ;
                                  rdfs:domain dici_core:GaseousFuelCarrier .

dici_core:hasHeatAttribute a owl:ObjectProperty ;
                           rdfs:subPropertyOf dici_core:hasThermalEnergyAttribute ;
                           rdfs:domain dici_core:HeatCarrier .

dici_core:hasHistoricTimeSeries a owl:ObjectProperty ;
                                rdfs:subPropertyOf dici_core:hasTimeSeries ;
                                owl:inverseOf dici_core:isHistoricTimeSeriesOf .

dici_core:hasInputAttribute a owl:ObjectProperty ;
                            owl:inverseOf dici_core:isAttributeOf ;
                            rdfs:domain dici_core:ServiceRequirement ;
                            rdfs:range dici_core:Attribute ;
                            rdfs:label "has input attribute" .

dici_core:hasInputEntity a owl:ObjectProperty ;
                         owl:inverseOf dici_core:isEntityOf ;
                         rdfs:domain dici_core:ComponentLink ,
                                     dici_core:ServiceRequirement ;
                         rdfs:range dici_core:Component ;
                         rdfs:label "has input entity" .

dici_core:hasInputFlow a owl:ObjectProperty ;
                       rdfs:subPropertyOf dici_core:linksComponent ;
                       rdfs:domain dici_core:Component ;
                       rdfs:range dici_core:Flow ;
                       rdfs:comment "Links a component to its input flows"@en ;
                       rdfs:label "has input flow" .

dici_core:hasJunctionAttribute a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                               rdfs:domain dici_core:Junction ;
                               rdfs:range dici_core:JunctionAttribute ;
                               rdfs:label "has junction attribute" .

dici_core:hasLiquidFuelAttribute a owl:ObjectProperty ;
                                 rdfs:subPropertyOf dici_core:hasFuelAttribute ;
                                 rdfs:domain dici_core:LiquidFuel .

dici_core:hasLiveTimeSeries a owl:ObjectProperty ;
                            rdfs:subPropertyOf dici_core:hasTimeSeries ;
                            owl:inverseOf dici_core:isLiveTimeSeriesOf .

dici_core:hasLocationAttribute a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                               rdfs:domain dici_core:Location ;
                               rdfs:range dici_core:LocationAttribute ;
                               rdfs:label "has location attribute" .

dici_core:hasMaterialConverterAttribute a owl:ObjectProperty ;
                                        rdfs:subPropertyOf dici_core:hasConverterAttribute ;
                                        rdfs:domain dici_core:MaterialConverter .

dici_core:hasMaterialFlowAttribute a owl:ObjectProperty ;
                                   rdfs:subPropertyOf dici_core:hasFlowAttribute ;
                                   rdfs:domain dici_core:MaterialFlow ;
                                   rdfs:range dici_core:MaterialFlowAttribute ;
                                   rdfs:label "has material flow attribute" .

dici_core:hasMeterAttribute a owl:ObjectProperty ;
                            rdfs:subPropertyOf dici_core:hasDeviceAttribute ;
                            rdfs:domain dici_core:Meter ;
                            rdfs:range dici_core:MeterAttribute ;
                            rdfs:label "has meter attribute" .

dici_core:hasNetworkAttribute a owl:ObjectProperty ;
                              rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                              rdfs:domain dici_core:Network ;
                              rdfs:range dici_core:NetworkAttribute ;
                              rdfs:label "has network attribute" .

dici_core:hasNonRenewableAttribute a owl:ObjectProperty ;
                                   rdfs:subPropertyOf dici_core:hasResourceAttribute ;
                                   rdfs:domain dici_core:NonRenewableResource .

dici_core:hasOutputFlow a owl:ObjectProperty ;
                        rdfs:subPropertyOf dici_core:linksComponent ;
                        rdfs:domain dici_core:Component ;
                        rdfs:range dici_core:Flow ;
                        rdfs:comment "Links a component to its output flows"@en ;
                        rdfs:label "has output flow" .

dici_core:hasPart a owl:ObjectProperty ;
                  rdfs:subPropertyOf dici_core:linksComponent ;
                  owl:inverseOf dici_core:partOf .

dici_core:hasProcessAttribute a owl:ObjectProperty ;
                              rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                              rdfs:domain dici_core:Process ;
                              rdfs:range dici_core:ProcessAttribute ;
                              rdfs:label "has process attribute" .

dici_core:hasRenewableAttribute a owl:ObjectProperty ;
                                rdfs:subPropertyOf dici_core:hasResourceAttribute ;
                                rdfs:domain dici_core:RenewableResource .

dici_core:hasResourceAttribute a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                               rdfs:domain dici_core:Resource .

dici_core:hasSensorAttribute a owl:ObjectProperty ;
                             rdfs:subPropertyOf dici_core:hasDeviceAttribute ;
                             rdfs:domain dici_core:Sensor ;
                             rdfs:range dici_core:SensorAttribute ;
                             rdfs:label "has sensor attribute" .

dici_core:hasSolarAttribute a owl:ObjectProperty ;
                            rdfs:subPropertyOf dici_core:hasRenewableAttribute ;
                            rdfs:domain dici_core:SolarResource .

dici_core:hasSolidFuelAttribute a owl:ObjectProperty ;
                                rdfs:subPropertyOf dici_core:hasFuelAttribute ;
                                rdfs:domain dici_core:SolidFuel .

dici_core:hasSource a owl:ObjectProperty ;
                    rdfs:subPropertyOf dici_core:linksComponent ;
                    owl:inverseOf dici_core:isSourceOf ;
                    rdfs:domain dici_core:Flow ;
                    rdfs:range dici_core:Component ;
                    rdfs:comment "The component from which the flow originates"@en ;
                    rdfs:label "has source" .

dici_core:hasStorageAttribute a owl:ObjectProperty ;
                              rdfs:subPropertyOf dici_core:hasComponentAttribute ;
                              rdfs:domain dici_core:Storage ;
                              rdfs:range dici_core:StorageAttribute ;
                              rdfs:label "has storage attribute" .

dici_core:hasSwitchAttribute a owl:ObjectProperty ;
                             rdfs:subPropertyOf dici_core:hasDeviceAttribute ;
                             rdfs:domain dici_core:Switch ;
                             rdfs:range dici_core:SwitchAttribute ;
                             rdfs:label "has switch attribute" .

dici_core:hasThermalEnergyAttribute a owl:ObjectProperty ;
                                    rdfs:subPropertyOf dici_core:hasEnergyCarrierAttribute ;
                                    rdfs:domain dici_core:ThermalEnergyCarrier .

dici_core:hasTimeSeries a owl:ObjectProperty ;
                        owl:inverseOf dici_core:isTimeSeriesOf ;
                        rdfs:domain dici_core:DynamicAttribute ;
                        rdfs:range dici_core:TimeSeries ;
                        rdfs:comment "Links a dynamic attribute to its associated time series data"@en ;
                        rdfs:label "has time series" .

dici_core:hasTransportProcessAttribute a owl:ObjectProperty ;
                                       rdfs:subPropertyOf dici_core:hasProcessAttribute ;
                                       rdfs:domain dici_core:TransportProcess ;
                                       rdfs:range dici_core:TransportProcessAttribute ;
                                       rdfs:label "has transport process attribute" .

dici_core:hasUnit a owl:ObjectProperty ;
                  rdfs:domain dici_core:Attribute ;
                  rdfs:range qudt:Unit ;
                  rdfs:label "has unit" .

dici_core:hasWindAttribute a owl:ObjectProperty ;
                           rdfs:subPropertyOf dici_core:hasRenewableAttribute ;
                           rdfs:domain dici_core:Wind .

dici_core:isAttributeOf a owl:ObjectProperty ;
                        rdfs:domain dici_core:Attribute ;
                        rdfs:range dici_core:ServiceRequirement ;
                        rdfs:label "is attribute of" .

dici_core:isDestinationOf a owl:ObjectProperty ;
                          rdfs:subPropertyOf dici_core:linksComponent ;
                          rdfs:domain dici_core:Component ;
                          rdfs:range dici_core:Flow ;
                          rdfs:label "is destination of" .

dici_core:isEntityOf a owl:ObjectProperty ;
                     rdfs:domain dici_core:Component ;
                     rdfs:range dici_core:ServiceRequirement ;
                     rdfs:label "is entity of" .

dici_core:isFutureTimeSeriesOf a owl:ObjectProperty ;
                               rdfs:subPropertyOf dici_core:isTimeSeriesOf ;
                               rdfs:domain dici_core:FutureTimeSeries ;
                               rdfs:range dici_core:DynamicAttribute .

dici_core:isHistoricTimeSeriesOf a owl:ObjectProperty ;
                                 rdfs:subPropertyOf dici_core:isTimeSeriesOf ;
                                 rdfs:domain dici_core:HistoricTimeSeries ;
                                 rdfs:range dici_core:DynamicAttribute .

dici_core:isLiveTimeSeriesOf a owl:ObjectProperty ;
                             rdfs:subPropertyOf dici_core:isTimeSeriesOf ;
                             rdfs:domain dici_core:LiveTimeSeries ;
                             rdfs:range dici_core:DynamicAttribute .

dici_core:isRequiredBy a owl:ObjectProperty ;
                       owl:inverseOf dici_core:requires ;
                       rdfs:domain dici_core:ServiceRequirement ;
                       rdfs:range dici_core:Service ;
                       rdfs:label "is required by" .

dici_core:isSourceOf a owl:ObjectProperty ;
                     rdfs:subPropertyOf dici_core:linksComponent ;
                     rdfs:domain dici_core:Component ;
                     rdfs:range dici_core:Flow ;
                     rdfs:label "is source of" .

dici_core:isTimeSeriesOf a owl:ObjectProperty ;
                         rdfs:domain dici_core:TimeSeries ;
                         rdfs:range dici_core:DynamicAttribute ;
                         rdfs:comment "Links a time series to the dynamic attribute it represents"@en ;
                         rdfs:label "is time series of" .

dici_core:linksComponent a owl:ObjectProperty ,
                                  owl:SymmetricProperty ;
                         rdfs:domain dici_core:Component ;
                         rdfs:range dici_core:Component .

dici_core:linksInputEntityTo a owl:ObjectProperty ;
                             rdfs:domain dici_core:ComponentLink ;
                             rdfs:range dici_core:Component .

dici_core:locatedAt a owl:ObjectProperty ;
                    rdfs:subPropertyOf dici_core:linksComponent ;
                    rdfs:domain dici_core:Component ;
                    rdfs:range dici_core:Location ;
                    rdfs:comment "Physical location of a component"@en ;
                    rdfs:label "located at" .

dici_core:locatedIn a owl:ObjectProperty ;
                    rdfs:subPropertyOf dici_core:linksComponent ;
                    owl:inverseOf dici_core:locationOf ;
                    rdfs:domain dici_core:Location .

dici_core:locationOf a owl:ObjectProperty ;
                     rdfs:subPropertyOf dici_core:linksComponent .

dici_core:measures a owl:ObjectProperty ;
                   rdfs:domain dici_core:Device ;
                   rdfs:range dici_core:Attribute ;
                   rdfs:comment "Device measures a specific attribute"@en ;
                   rdfs:label "measures" .

dici_core:monitors a owl:ObjectProperty ;
                   rdfs:subPropertyOf dici_core:linksComponent ;
                   rdfs:domain dici_core:Device ;
                   rdfs:range dici_core:Component ;
                   rdfs:comment "Device monitors another component"@en ;
                   rdfs:label "monitors" .

dici_core:operates a owl:ObjectProperty ;
                   rdfs:subPropertyOf dici_core:linksComponent ;
                   rdfs:domain dici_core:Actor ;
                   rdfs:range dici_core:Component ;
                   rdfs:comment "Operational control relationship"@en ;
                   rdfs:label "operates" .

dici_core:owns a owl:ObjectProperty ;
               rdfs:subPropertyOf dici_core:linksComponent ;
               rdfs:domain dici_core:Actor ;
               rdfs:range dici_core:Component ;
               rdfs:comment "Ownership relationship between actors and components"@en ;
               rdfs:label "owns" .

dici_core:partOf a owl:ObjectProperty ;
                 rdfs:subPropertyOf dici_core:linksComponent .

dici_core:realTimeSource a owl:ObjectProperty ;
                         rdfs:domain dici_core:DynamicAttribute ;
                         rdfs:comment "Reference to a real-time data source for live dynamic attributes"@en ;
                         rdfs:label "real-time source" .

dici_core:requires a owl:ObjectProperty ;
                   rdfs:domain dici_core:Service ;
                   rdfs:range dici_core:ServiceRequirement ;
                   rdfs:label "requires" .

dici_core:targetAttribute a owl:ObjectProperty ;
                          rdfs:subPropertyOf dici_core:assumptionObjectProperty ;
                          rdfs:domain dici_core:Assumption ;
                          rdfs:range dici_core:Attribute .

dici_core:targetComponent a owl:ObjectProperty ;
                          rdfs:subPropertyOf dici_core:assumptionObjectProperty ;
                          rdfs:domain dici_core:Assumption ;
                          rdfs:range dici_core:Component .

dici_core:usedInScenario a owl:ObjectProperty ;
                         rdfs:domain dici_core:Attribute ,
                                     dici_core:Component ,
                                     dici_core:ComponentLink ;
                         rdfs:range dici_core:Scenario .

dici_core:xAttribute a owl:ObjectProperty ;
                     rdfs:domain dici_core:CurveAttribute ;
                     rdfs:range qudt:QuantityKind ;
                     rdfs:label "x attribute" .

dici_core:xUnit a owl:ObjectProperty ;
                rdfs:subPropertyOf dici_core:hasUnit ;
                rdfs:domain dici_core:CurveAttribute ;
                rdfs:range qudt:Unit ;
                rdfs:label "x unit" .

dici_core:yAttribute a owl:ObjectProperty ;
                     rdfs:domain dici_core:CurveAttribute ;
                     rdfs:range qudt:QuantityKind ;
                     rdfs:label "y attribute" .

dici_core:yUnit a owl:ObjectProperty ;
                rdfs:subPropertyOf dici_core:hasUnit ;
                rdfs:domain dici_core:CurveAttribute ;
                rdfs:range qudt:Unit ;
                rdfs:label "y unit" .

qudt:numericValue a owl:DatatypeProperty .

qudt:value a owl:DatatypeProperty ;
           rdfs:subPropertyOf dici_core:hasAttributeValue ;
           rdfs:domain dici_core:Attribute .

dici_core:assumptionDataProperty a owl:DatatypeProperty .

dici_core:assumptionTimesteps a owl:DatatypeProperty ;
                              rdfs:subPropertyOf dici_core:assumptionDataProperty ;
                              rdfs:domain dici_core:AssumptionSeries .

dici_core:endTime a owl:DatatypeProperty ;
                  rdfs:domain dici_core:TimeSeries ;
                  rdfs:range xsd:dateTime ;
                  rdfs:comment "The ending timestamp of the time series data"@en ;
                  rdfs:label "end time" .

dici_core:hasAttributeValue a owl:DatatypeProperty ;
                            rdfs:domain dici_core:Attribute ;
                            rdfs:label "has attribute value" .

dici_core:hasDataPath a owl:DatatypeProperty ;
                      rdfs:subPropertyOf dici_core:hasAttributeValue ;
                      rdfs:domain dici_core:CurveAttribute ;
                      rdfs:range xsd:string ;
                      rdfs:label "has data path" .

dici_core:hasDataPoints a owl:DatatypeProperty ;
                        rdfs:subPropertyOf dici_core:hasAttributeValue .

dici_core:hasFileName a owl:DatatypeProperty ;
                      rdfs:domain dici_core:TimeSeries ;
                      rdfs:range xsd:string ;
                      rdfs:comment "The filename of the time series data file"@en ;
                      rdfs:label "has file name" .

dici_core:modifier a owl:DatatypeProperty ;
                   rdfs:subPropertyOf dici_core:assumptionDataProperty .

dici_core:modifierValue a owl:DatatypeProperty ;
                        rdfs:subPropertyOf dici_core:assumptionDataProperty .

dici_core:populationInfluenced a owl:DatatypeProperty ;
                               rdfs:subPropertyOf dici_core:assumptionDataProperty .

dici_core:startTime a owl:DatatypeProperty ;
                    rdfs:domain dici_core:TimeSeries ;
                    rdfs:range xsd:dateTime ;
                    rdfs:comment "The starting timestamp of the time series data"@en ;
                    rdfs:label "start time" .

dici_core:storedAt a owl:DatatypeProperty ;
                   rdfs:domain dici_core:TimeSeries ;
                   rdfs:range xsd:string ;
                   rdfs:comment "The location or path where the time series data is stored"@en ;
                   rdfs:label "stored at" .

dici_core:temporalResolution a owl:DatatypeProperty ;
                             rdfs:domain dici_core:TimeSeries ;
                             rdfs:range xsd:duration ;
                             rdfs:comment "The time interval between data points in the time series (e.g., PT1H for hourly, PT15M for 15-minute intervals)"@en ;
                             rdfs:label "temporal resolution" .

dici_core:timeSeriesType a owl:DatatypeProperty ;
                         rdfs:domain dici_core:TimeSeries ;
                         rdfs:range xsd:string ;
                         rdfs:comment "The type of time series: 'Historic', 'Live', or 'Future'"@en ;
                         rdfs:label "time series type" .

qudt:QuantityKind a owl:Class .

qudt:Unit a owl:Class .

rdf:JSON a owl:Class .

dici_core:Actor a owl:Class ;
                rdfs:subClassOf dici_core:Component ;
                rdfs:comment "Human or organizational agent that owns, operates, or regulates components"@en ;
                rdfs:label "Actor" ;
                dici_core:definition "Actors represent stakeholders in the energy system including operators, regulators, consumers, and policy makers"@en .

dici_core:ActorAttribute a owl:Class ;
                         rdfs:subClassOf dici_core:ComponentAttribute ;
                         rdfs:label "Actor Attribute" .

dici_core:Actuator a owl:Class ;
                   rdfs:subClassOf dici_core:Device ;
                   rdfs:comment "Device that converts control signals into physical action"@en ;
                   rdfs:label "Actuator" ;
                   dici_core:definition "Actuators are devices that produce motion or control mechanisms based on input signals"@en .

dici_core:ActuatorAttribute a owl:Class ;
                            rdfs:subClassOf dici_core:DeviceAttribute ;
                            rdfs:label "Actuator Attribute" .

dici_core:ActuatorPosition a owl:Class ;
                           rdfs:subClassOf dici_core:ActuatorAttribute ,
                                           dici_core:DynamicAttribute ;
                           rdfs:comment "Current position or state of an actuator"@en ;
                           rdfs:label "Actuator Position" ;
                           dici_core:hasDefaultUnit unit:PERCENT .

dici_core:Assumption a owl:Class .

dici_core:AssumptionSeries a owl:Class ;
                           rdfs:subClassOf dici_core:Assumption .

dici_core:AssumptionSingle a owl:Class ;
                           rdfs:subClassOf dici_core:Assumption .

dici_core:Attribute a owl:Class ;
                    rdfs:subClassOf owl:Thing ;
                    rdfs:label "Attribute" .

dici_core:CircuitBreaker a owl:Class ;
                         rdfs:subClassOf dici_core:Switch ;
                         rdfs:comment "Switch that automatically interrupts electrical flow for safety"@en ;
